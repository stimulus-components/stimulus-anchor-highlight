    "test": "BABEL_ENV=test jest",
    "np": "np --no-2fa"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/plugin-syntax-class-properties": "7.12.13",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@hotwired/stimulus": "^3.1.2",
    "np": "^7.6.2",
    "prettier-standard": "16.4.1",
    "typescript": "^4.8.4",
    "vite": "^2.6.0"
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.1.2"
  }
}
