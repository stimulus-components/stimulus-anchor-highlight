{
  "name": "stimulus-anchor-highlight",
  "version": "0.0.0",
  "description": "A Stimulus controller that highlights element matching the anchor.",
  "keywords": [
    "stimulus",
    "stimulusjs",
    "stimulus controller",
    "anchor highlight"
  ],
  "repository": "git@github.com:stimulus-components/stimulus-anchor-highlight.git",
  "bugs": {
    "url": "https://github.com/stimulus-components/stimulus-anchor-highlight/issues"
  },
  "author": "Guillaume Briday <guillaumebriday@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/stimulus-components/stimulus-anchor-highlight",
  "private": false,
  "main": "dist/stimulus-anchor-highlight.umd.js",
  "module": "dist/stimulus-anchor-highlight.es.js",
  "scripts": {
    "format": "prettier-standard  '**/*.{ts,css,html}' --format",
    "lint": "prettier-standard  '**/*.{ts,css,html}' --lint",
    "dev": "vite",
    "prod": "vite build --config /dev/null",
    "build": "tsc --noEmit && vite build",
